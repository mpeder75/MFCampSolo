{
  "ReverseProxy": {
    "Routes": {
      "paymentRoute": {
        "ClusterId": "paymentCluster",
        "Match": {
          "Path": "/payment/{**catch-all}"
        }
      },
      "customerRoute": {
        "ClusterId": "customerCluster",
        "Match": {
          "Path": "/customer/{**catch-all}"
        }
      },
      "warehouseRoute": {
        "ClusterId": "warehouseCluster",
        "Match": {
          "Path": "/warehouse/{**catch-all}"
        }
      },
      "orderRoute": {
        "ClusterId": "orderCluster",
        "Match": {
          "Path": "/order/{**catch-all}"
        }
      },
      "shippingRoute": {
        "ClusterId": "shippingCluster",
        "Match": {
          "Path": "/shipping/{**catch-all}"
        }
      },
      "workflowRoute": {
        "ClusterId": "workflowCluster",
        "Match": {
          "Path": "/workflow/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "paymentCluster": {
        "Destinations": {
          "paymentDestination": {
            "Address": "http://localhost:3501"
          }
        }
      },
      "customerCluster": {
        "Destinations": {
          "customerDestination": {
            "Address": "http://localhost:3505"
          }
        }
      },
      "warehouseCluster": {
        "Destinations": {
          "warehouseDestination": {
            "Address": "http://localhost:3503"
          }
        }
      },
      "orderCluster": {
        "Destinations": {
          "orderDestination": {
            "Address": "http://localhost:3502"
          }
        }
      },
      "shippingCluster": {
        "Destinations": {
          "shippingDestination": {
            "Address": "http://localhost:3504"
          }
        }
      },
      "workflowCluster": {
        "Destinations": {
          "workflowDestination": {
            "Address": "http://localhost:3506"
          }
        }
      }
    },
    "Keycloak": {
      "Authority": "https://your-keycloak-server/auth/realms/your-realm",
      "ClientId": "public-client"
    }
  }
}
